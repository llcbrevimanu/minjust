{
  "name": "somework/minjust",
  "description": "Service to get data from minjust",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Igor Pinchuk",
      "email": "i.pinchuk.work@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SomeWork\\Minjust\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SomeWork\\Minjust\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "php-cs-fixer fix --diff --dry-run",
    "fix": "php-cs-fixer fix"
  },
  "require": {
    "php": "^7.2",
    "psr/http-message": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "paquettg/php-html-parser": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "phpunit/phpunit": "^8.4",
    "http-interop/http-factory-guzzle": "^1.0",
    "divineomega/psr-18-guzzle-adapter": "^1.0"
  }
}
